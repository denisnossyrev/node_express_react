  "bugs": {
    "url": "https://github.com/denisnossyrev/node_express_react/issues"
  },
  "homepage": "https://github.com/denisnossyrev/node_express_react#readme",
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.18.2",
    "eslint-plugin-react": "^7.7.0",
    "webpack-cli": "^2.0.10"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bootstrap": "^4.0.0",
